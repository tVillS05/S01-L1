package main

import "fmt"

func fibonacci(n int) {
	var a int = 0
	var b int = 1

	for i := 0; i < n; i++ {
		fmt.Print(a, " ")
		a, b = b, a+b
	}
	fmt.Println()
}

func main() {
	var n int

	fmt.Print("Digite quantos números da sequência de Fibonacci você quer: ")
	fmt.Scanln(&n)

	fibonacci(n)
}
